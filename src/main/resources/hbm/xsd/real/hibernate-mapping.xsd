<!--
    Hibernate Mapping DTD.
    
    <!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    An instance of this XML document may contain org.hibernate.mappings for an arbitrary
    number of classes. The class org.hibernate.mappings may contain associations to classes
    mapped in the same document or in another document. No class may be 
    mapped more than once. Each document may also contain definitions of an
    arbitrary number of queries, and import declarations of arbitrary classes. 
    
-->
<!-- The document root. -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           xmlns:jxb="https://jakarta.ee/xml/ns/jaxb"
           jxb:version="3.0"
           elementFormDefault="qualified">
    <xsd:annotation>
        <xsd:appinfo>
            <jxb:schemaBindings>
                <jxb:nameXmlTransform>
                    <jxb:elementName prefix="Hbm"></jxb:elementName>
                </jxb:nameXmlTransform>
            </jxb:schemaBindings>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:element name="hibernate-mapping">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="identifier-generator"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="typedef"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="filter-def"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="import"/>
                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element ref="class"/>
                    <xsd:element ref="subclass"/>
                    <xsd:element ref="joined-subclass"/>
                    <xsd:element ref="union-subclass"/>
                </xsd:choice>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="resultset"/>
                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element ref="query"/>
                    <xsd:element ref="sql-query"/>
                </xsd:choice>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="fetch-profile"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="database-object"/>
            </xsd:sequence>
            <xsd:attribute name="schema"/>
            <xsd:attribute name="catalog"/>
            <xsd:attribute name="default-cascade" default="none"/>
            <xsd:attribute name="default-access" default="property"/>
            <xsd:attribute name="default-lazy" default="true">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="auto-import" default="true">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="package"/>
        </xsd:complexType>
    </xsd:element>
    <!-- default: none -->
    <!-- default: none -->
    <!-- default: none -->
    <!--
        <meta.../> is used to assign meta-level attributes to a class
        or property.  Is currently used by codegenerator as a placeholder for
        values that is not directly related to OR org.hibernate.mappings.
    -->
    <xsd:element name="meta">
        <xsd:complexType mixed="true">
            <xsd:attribute name="attribute" use="required"/>
            <xsd:attribute name="inherit" default="true">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!-- <identifier-generator.../> allows customized short-naming of IdentifierGenerator implementations. -->
    <xsd:element name="identifier-generator">
        <xsd:complexType>
            <xsd:attribute name="name" use="required"/>
            <xsd:attribute name="class" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <!--
        <typedef.../> allows defining a customized type mapping for a Hibernate type. May
        contain parameters for parameterizable types.
    -->
    <xsd:element name="typedef">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="param"/>
            </xsd:sequence>
            <xsd:attribute name="class" use="required"/>
            <xsd:attribute name="name" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <!-- IMPORT element definition; an explicit query language "import" -->
    <xsd:element name="import">
        <xsd:complexType>
            <xsd:attribute name="class" use="required"/>
            <xsd:attribute name="rename"/>
        </xsd:complexType>
    </xsd:element>
    <!-- default: unqualified class name -->
    <!--
        Root entity mapping.  Poorly named as entities do not have to be represented by 
        classes at all.  Mapped entities may be represented via different methodologies 
        (POJO, Map, Dom4j).
    -->
    <xsd:element name="class">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
                <xsd:element minOccurs="0" ref="subselect"/>
                <xsd:element minOccurs="0" ref="cache"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="synchronize"/>
                <xsd:element minOccurs="0" ref="comment"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="tuplizer"/>
                <xsd:choice>
                    <xsd:element ref="id"/>
                    <xsd:element ref="composite-id"/>
                </xsd:choice>
                <xsd:element minOccurs="0" ref="discriminator"/>
                <xsd:element minOccurs="0" ref="natural-id"/>
                <xsd:choice minOccurs="0">
                    <xsd:element ref="version"/>
                    <xsd:element ref="timestamp"/>
                </xsd:choice>
                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element ref="property"/>
                    <xsd:element ref="many-to-one"/>
                    <xsd:element ref="one-to-one"/>
                    <xsd:element ref="component"/>
                    <xsd:element ref="dynamic-component"/>
                    <xsd:element ref="properties"/>
                    <xsd:element ref="any"/>
                    <xsd:element ref="map"/>
                    <xsd:element ref="set"/>
                    <xsd:element ref="list"/>
                    <xsd:element ref="bag"/>
                    <xsd:element ref="idbag"/>
                    <xsd:element ref="array"/>
                    <xsd:element ref="primitive-array"/>
                </xsd:choice>
                <xsd:choice>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" maxOccurs="unbounded" ref="join"/>
                        <xsd:element minOccurs="0" maxOccurs="unbounded" ref="subclass"/>
                    </xsd:sequence>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" ref="joined-subclass"/>
                    <xsd:element minOccurs="0" maxOccurs="unbounded" ref="union-subclass"/>
                </xsd:choice>
                <xsd:element minOccurs="0" ref="loader"/>
                <xsd:element minOccurs="0" ref="sql-insert"/>
                <xsd:element minOccurs="0" ref="sql-update"/>
                <xsd:element minOccurs="0" ref="sql-delete"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="filter"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="fetch-profile"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="resultset"/>
                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element ref="query"/>
                    <xsd:element ref="sql-query"/>
                </xsd:choice>
            </xsd:sequence>
            <xsd:attribute name="entity-name"/>
            <xsd:attribute name="name"/>
            <xsd:attribute name="proxy"/>
            <xsd:attribute name="lazy">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="table"/>
            <xsd:attribute name="schema"/>
            <xsd:attribute name="catalog"/>
            <xsd:attribute name="subselect">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="valueAttribute" />
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="discriminator-value"/>
            <xsd:attribute name="mutable" default="true">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="abstract">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="polymorphism" default="implicit">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="implicit"/>
                        <xsd:enumeration value="explicit"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="where"/>
            <xsd:attribute name="persister"/>
            <xsd:attribute name="dynamic-update" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="dynamic-insert" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="batch-size"/>
            <xsd:attribute name="select-before-update" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="optimistic-lock" default="version">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="none"/>
                        <xsd:enumeration value="version"/>
                        <xsd:enumeration value="dirty"/>
                        <xsd:enumeration value="all"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="check"/>
            <xsd:attribute name="rowid"/>
            <xsd:attribute name="node"/>
        </xsd:complexType>
    </xsd:element>
    <!-- this is the class name -->
    <!-- default: no proxy interface -->
    <!-- default: unqualified classname -->
    <!-- default: none -->
    <!-- default: none -->
    <!-- default: unqualified class name | none -->
    <!-- default: none -->
    <!-- default: none -->
    <!-- TUPLIZER element; defines tuplizer to use for a component/entity for a given entity-mode -->
    <xsd:element name="tuplizer">
        <xsd:complexType>
            <xsd:attribute name="entity-mode">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="pojo"/>
                        <xsd:enumeration value="dom4j"/>
                        <xsd:enumeration value="dynamic-map"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="class" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <!-- entity mode for which tuplizer is in effect -->
    <!-- the tuplizer class to use -->
    <!-- FILTER-DEF element; top-level filter definition. -->
    <xsd:element name="filter-def">
        <xsd:complexType mixed="true">
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="filter-param"/>
            </xsd:sequence>
            <xsd:attribute name="name" use="required"/>
            <xsd:attribute name="condition"/>
        </xsd:complexType>
    </xsd:element>
    <!-- The filter name -->
    <!--
        FILTER-PARAM element; qualifies parameters found within a FILTER-DEF
        condition.
    -->
    <xsd:element name="filter-param">
        <xsd:complexType>
            <xsd:attribute name="name" use="required"/>
                        <xsd:attribute name="type" use="required">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="typeAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!-- The parameter name -->
    <!-- The parameter type -->
    <!-- FILTER element; used to apply a filter. -->
    <xsd:element name="filter">
        <xsd:complexType mixed="true">
            <xsd:attribute name="name" use="required"/>
            <xsd:attribute name="condition"/>
        </xsd:complexType>
    </xsd:element>
    <!---->
    <xsd:element name="fetch-profile">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="fetch"/>
            </xsd:sequence>
            <xsd:attribute name="name" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <!--
        The <fetch> element defines a single path to which the fetch
        refers, as well as the style of fetch to apply.  The 'root' of the
        path is different depending upon the context in which the
        containing <fetch-profile/> occurs; within a <class/> element,
        the entity-name of the containing class mapping is assumed...
    -->
    <xsd:element name="fetch">
        <xsd:complexType>
            <xsd:attribute name="entity"/>
            <xsd:attribute name="association" use="required"/>
            <xsd:attribute name="style" default="join">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="join"/>
                        <xsd:enumeration value="select"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!-- Implied as long as the containing fetch profile is contained in a class mapping -->
    <!-- A join allows some properties of a class to be persisted to a second table -->
    <xsd:element name="join">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" ref="subselect"/>
                <xsd:element minOccurs="0" ref="comment"/>
                <xsd:element ref="key"/>
                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element ref="property"/>
                    <xsd:element ref="many-to-one"/>
                    <xsd:element ref="component"/>
                    <xsd:element ref="dynamic-component"/>
                    <xsd:element ref="any"/>
                </xsd:choice>
                <xsd:element minOccurs="0" ref="sql-insert"/>
                <xsd:element minOccurs="0" ref="sql-update"/>
                <xsd:element minOccurs="0" ref="sql-delete"/>
            </xsd:sequence>
            <xsd:attribute name="table" use="required"/>
            <xsd:attribute name="schema"/>
            <xsd:attribute name="catalog"/>
            <xsd:attribute name="subselect">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="subselectAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="fetch" default="join">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="join"/>
                        <xsd:enumeration value="select"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="inverse" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="optional" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!-- default: none -->
    <!-- default: none -->
    <!-- A natural-id element allows declaration of the unique business key -->
    <xsd:element name="natural-id">
        <xsd:complexType>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element ref="property"/>
                <xsd:element ref="many-to-one"/>
                <xsd:element ref="component"/>
                <xsd:element ref="dynamic-component"/>
                <xsd:element ref="any"/>
            </xsd:choice>
            <xsd:attribute name="mutable" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!--
        Declares the id type, column and generation algorithm for an entity class.
        If a name attribut is given, the id is exposed to the application through the 
        named property of the class. If not, the id is only exposed to the application 
        via Session.getIdentifier()
    -->
    <xsd:element name="id">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="column"/>
                <xsd:element minOccurs="0" ref="type"/>
                <xsd:element minOccurs="0" ref="generator"/>
            </xsd:sequence>
            <xsd:attribute name="name"/>
            <xsd:attribute name="node"/>
            <xsd:attribute name="access"/>
            <xsd:attribute name="column" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="columnAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
                        <xsd:attribute name="type">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="typeAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="length"/>
            <xsd:attribute name="unsaved-value"/>
        </xsd:complexType>
    </xsd:element>
    <!-- any|none|null|undefined|0|-1|... -->
    <!--
        A composite key may be modelled by a java class with a property for each 
        key column. The class must implement java.io.Serializable and reimplement equals() 
        and hashCode().
    -->
    <xsd:element name="composite-id">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element ref="key-property"/>
                    <xsd:element ref="key-many-to-one"/>
                </xsd:choice>
                <xsd:element minOccurs="0" ref="generator"/>
            </xsd:sequence>
            <xsd:attribute name="class"/>
            <xsd:attribute name="mapped" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="name"/>
            <xsd:attribute name="node"/>
            <xsd:attribute name="access"/>
            <xsd:attribute name="unsaved-value" default="undefined">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="undefined"/>
                        <xsd:enumeration value="any"/>
                        <xsd:enumeration value="none"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!--
        Polymorphic data requires a column holding a class discriminator value. This
        value is not directly exposed to the application.
    -->
    <xsd:element name="discriminator">
        <xsd:complexType>
            <xsd:choice minOccurs="0">
                <xsd:element ref="column"/>
                <xsd:element ref="formula"/>
            </xsd:choice>
            <xsd:attribute name="column" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="columnAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
                        <xsd:attribute name="formula">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="formulaAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="type" default="string"/>
            <xsd:attribute name="not-null" default="true">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="length"/>
            <xsd:attribute name="force" default="false">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="insert" default="true">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!-- default: "class"|none -->
    <!--
        Versioned data requires a column holding a version number. This is exposed to the
        application through a property of the Java class.
    -->
    <xsd:element name="version">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="column"/>
            </xsd:sequence>
            <xsd:attribute name="name" use="required"/>
            <xsd:attribute name="node"/>
            <xsd:attribute name="access"/>
            <xsd:attribute name="column" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="columnAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="type" default="integer"/>
            <xsd:attribute name="unsaved-value" default="undefined">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="null"/>
                        <xsd:enumeration value="negative"/>
                        <xsd:enumeration value="undefined"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="generated" default="never">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="never"/>
                        <xsd:enumeration value="always"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="insert">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="true"/>
                        <xsd:enumeration value="false"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="timestamp">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
            </xsd:sequence>
            <xsd:attribute name="name" use="required"/>
            <xsd:attribute name="node"/>
            <xsd:attribute name="column" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jxb:property name="columnAttr"></jxb:property>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="access"/>
            <xsd:attribute name="unsaved-value" default="null">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="null"/>
                        <xsd:enumeration value="undefined"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="source" default="vm">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="vm"/>
                        <xsd:enumeration value="db"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="generated" default="never">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="never"/>
                        <xsd:enumeration value="always"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!--
        Subclass declarations are nested beneath the root class declaration to achieve
        polymorphic persistence with the table-per-hierarchy mapping strategy.
        
        See the note on the class element regarding <pojo/> vs. @name usage...
    -->
    <xsd:element name="subclass">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="tuplizer"/>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="synchronize"/>
                <xsd:choice minOccurs="0" maxOccurs="unbounded">
                    <xsd:element ref="property"/>
                    <xsd:element ref="many-to-one"/>
                    <xsd:element ref="one-to-one"/>
                    <xsd:element ref="component"/>
                    <xsd:element ref="dynamic-component"/>
                    <xsd:element ref="any"/>
                    <xsd:element ref="map"/>
                    <xsd:element ref="set"/>
                    <xsd:element ref="list"/>
                    <xsd:element ref="bag"/>
